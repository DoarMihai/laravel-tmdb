{
    "name": "doarmihai/laravel-tmdb",
    "description": "Laravel Package for TMDB ( The Movie Database ) API. Provides easy access to the TMDB api.",
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-0" : {
            "MihaiStefanescu\\TmdbApi" : "src"
        }
    },
    "authors": [
        {
            "name": "Stefanescu Mihai",
            "email": "mihai@invata-programare.ro"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MihaiStefanescu\\LaravelTmdb\\Providers\\TmdbServiceProvider"
            ],
            "aliases": {
                "Certification": "MihaiStefanescu\\LaravelTmdb\\Facade\\Certification",
                "Changes": "MihaiStefanescu\\LaravelTmdb\\Facade\\Changes",
                "Collections": "MihaiStefanescu\\LaravelTmdb\\Facade\\Collections",
                "Companies": "MihaiStefanescu\\LaravelTmdb\\Facade\\Companies",
                "Credits": "MihaiStefanescu\\LaravelTmdb\\Facade\\Credits",
                "Discover": "MihaiStefanescu\\LaravelTmdb\\Facade\\Discover",
                "Find": "MihaiStefanescu\\LaravelTmdb\\Facade\\Find",
                "Genres": "MihaiStefanescu\\LaravelTmdb\\Facade\\Genres",
                "Jobs": "MihaiStefanescu\\LaravelTmdb\\Facade\\Jobs",
                "Keyword": "MihaiStefanescu\\LaravelTmdb\\Facade\\Keyword",
                "Lists": "MihaiStefanescu\\LaravelTmdb\\Facade\\Lists",
                "Movie": "MihaiStefanescu\\LaravelTmdb\\Facade\\Movie",
                "Network": "MihaiStefanescu\\LaravelTmdb\\Facade\\Network",
                "People": "MihaiStefanescu\\LaravelTmdb\\Facade\\People",
                "Review": "MihaiStefanescu\\LaravelTmdb\\Facade\\Review",
                "Search": "MihaiStefanescu\\LaravelTmdb\\Facade\\Search",
                "Tv": "MihaiStefanescu\\LaravelTmdb\\Facade\\Tv",
                "TvEpisode": "MihaiStefanescu\\LaravelTmdb\\Facade\\TvEpisode",
                "TvEpisodeGroup": "MihaiStefanescu\\LaravelTmdb\\Facade\\TvEpisodeGroup",
                "TvSeason": "MihaiStefanescu\\LaravelTmdb\\Facade\\TvSeason"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
